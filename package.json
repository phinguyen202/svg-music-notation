{
  "name": "svg-music-notation",
  "version": "0.1.0",
  "description": "draw sheet music on browser using svg",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --port 9000 --open --hot --inline --content-base dev",
    "tscw": "rmdir /Q /S dist && ttsc && cp -R src/fonts dist/src/fonts ",
    "tscm": "rm -r dist && ttsc && npm run copyfiles",
    "copyfiles": "cp -R src/fonts dist/src/fonts && cp -R src/css dist/src/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phinguyen202/svg-sheet-music.git"
  },
  "keywords": [
    "svg",
    "music",
    "musical",
    "notation"
  ],
  "author": "Phi Trung Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phinguyen202/svg-sheet-music/issues"
  },
  "homepage": "https://github.com/phinguyen202/svg-sheet-music#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "css-loader": "^5.2.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "file-loader": "^6.2.0",
    "raw-loader": "^4.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^2.0.0",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.4",
    "typescript-transform-paths": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "source-renderer": "^0.2.2"
  }
}
